<?xml version="1.0" encoding="utf-8"?>
<merge xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical"
    android:gravity="center"
    android:baselineAligned="false"
    tools:parentTag="android.widget.LinearLayout">

    <ImageView
        android:id="@+id/iv_icon"
        android:layout_width="@dimen/spsdk_dp_40"
        android:layout_height="@dimen/spsdk_dp_40"
        android:scaleType="centerInside"
        android:src="@mipmap/spsdk_amend_record"/>

    <TextView
        android:id="@+id/title"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_margin="@dimen/spsdk_dp_5"
        android:textColor="@color/spsdk_main_color"
        android:textSize="@dimen/spsdk_ts_16"
        android:text="标签"/>

</merge>