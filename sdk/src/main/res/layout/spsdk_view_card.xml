<androidx.cardview.widget.CardView xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:layout_width="match_parent"
    android:layout_height="@dimen/spsdk_dp_35"
    android:layout_marginStart="@dimen/spsdk_dp_10"
    android:layout_marginTop="@dimen/spsdk_dp_10"
    android:layout_marginEnd="@dimen/spsdk_dp_10"
    android:layout_marginBottom="@dimen/spsdk_dp_10"
    app:cardCornerRadius="@dimen/spsdk_dp_5"
    app:cardElevation="@dimen/spsdk_dp_5">

    <EditText
        android:id="@+id/et_edit"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:gravity="center_vertical"
        android:paddingLeft="@dimen/spsdk_dp_10"
        android:hint="@string/app_name"
        android:background="@null"
        android:textSize="@dimen/spsdk_ts_14"
        />

</androidx.cardview.widget.CardView>


