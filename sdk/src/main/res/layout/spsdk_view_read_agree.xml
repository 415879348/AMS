<?xml version="1.0" encoding="utf-8"?>
<merge
    xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    xmlns:tools="http://schemas.android.com/tools"
    tools:parentTag="android.widget.FrameLayout">

    <ImageView
        android:id="@+id/read_agree_check"
        android:layout_width="@dimen/spsdk_dp_30"
        android:layout_height="@dimen/spsdk_dp_20"
        android:paddingStart="@dimen/spsdk_dp_10"
        android:textSize="@dimen/spsdk_ts_12"
        android:src="@drawable/spsdk_selector_check"
        tools:ignore="RtlSymmetry" />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:paddingTop="@dimen/spsdk_dp_2"
        android:paddingStart="@dimen/spsdk_dp_30"
        android:text="*"
        android:textColor="@color/spsdk_color_red"
        tools:ignore="RtlSymmetry" />

    <TextView
        android:id="@+id/read_agree_hint"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:paddingTop="@dimen/spsdk_dp_2"
        android:textSize="@dimen/spsdk_ts_12"
        android:textColor="@color/spsdk_main_color"
        android:lineSpacingMultiplier="1.3"/>

</merge>